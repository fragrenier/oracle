# This file is managed by Chef
# tnsnames.ora Network Configuration File: <%= @filename %>

<% @tnsnames_entry.each do |name, config| %>
<%= name %> =
  (DESCRIPTION =
   (ADDRESS_LIST =
      (ADDRESS = (PROTOCOL = TCP)
                 (HOST = <%= config.host %>)
                 (PORT = <%= config.port %>)
      )
    )
    (CONNECT_DATA =
      (SERVER = DEDICATED)
      (SERVICE_NAME = <%= config.service_name %>)
    )
  )
<% end %>
